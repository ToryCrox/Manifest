  <project path="common/mimikkoui_desktop_resolver_library" name="mimikkoui_desktop_resolver_library" />
  <project path="common/processor" name="processor" />
  
  
  <project path="features/checkin_feature" name="checkin_feature" />
  <project path="features/information_feature" name="information_feature" />
  <!-- <project path="features/launcher_core_feature" name="launcher_core_feature" /> -->
  <project path="features/launcher_info_assistant_feature" name="launcher_info_assistant_feature" />
  <project path="features/launcher_init_feature" name="launcher_init_feature" />
  <project path="features/launcher_settings_feature" name="launcher_settings_feature" />
  <project path="features/launcher3_core_feature" name="launcher3_core_feature" />
  <project path="features/schedule_feature" name="schedule_feature" />
  <project path="features/servant_feature" name="servant_feature" />
  <project path="features/share_feature" name="share_feature" />
